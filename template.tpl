___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Load template APIs
const copyFromWindow = require('copyFromWindow');
const enc = require('encodeUriComponent');
const injectScript = require('injectScript');
const log = require('logToConsole');
const makeTableMap = require('makeTableMap');

const SCRIPT_URL = 'https://fast.appcues.com/';
const LOG_PREFIX = '[Appcues / GTM] ';

const userProps = data.userProperties && data.userProperties.length ? makeTableMap(data.userProperties, 'name', 'value') : {};
const eventProps = data.eventProperties && data.eventProperties.length ? makeTableMap(data.eventProperties, 'name', 'value') : {};

const fail = msg => {
  log(LOG_PREFIX + msg);
  data.gtmOnFailure();
};

const onsuccess = () => {
  const Appcues = copyFromWindow('Appcues');
  if (!Appcues) return fail('Failed to load window.Appcues');

  switch (data.eventType) {
    case 'clearShow':
      Appcues.clearShow();
      break;
    case 'identify':
      if (data.anonymous) {
        Appcues.anonymous();
      } else {
        Appcues.identify(
          data.userId,
          userProps
        );
      }
      break;
    case 'page':
      Appcues.page();
      break;
    case 'reset':
      Appcues.reset();
      break;
    case 'show':
      Appcues.show(data.contentId);
      break;
    case 'track':
      Appcues.track(
        data.eventName,
        eventProps
      );
      break;
    default:
      break;
  }
    
  data.gtmOnSuccess();
};
injectScript(SCRIPT_URL + enc(data.accountId) + '.js', onsuccess, data.gtmOnFailure, data.accountId + '_appcues');


